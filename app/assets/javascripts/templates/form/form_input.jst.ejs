<input data-input="form" type="text" id="{{=object}}_{{=attribute}}" name="{{=object}}[{{=attribute}}]"
{{ for(var rule in rules) { }}
  {{ if(rules.hasOwnProperty(rule)) { }}
    data-rule-{{=rule}}={{=rules[rule]}}
  {{ } }}
{{ } }}

{{ if (disabled) { }}
  disabled="true"
{{ } }}

value="{{=value}}"

></input>